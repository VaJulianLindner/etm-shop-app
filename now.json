{
    "version": 2,
    "builds": [{ "src": "server/index.js", "use": "@now/node-server" }],
    "routes": [
        {
            "src": "/",
            "dest": "/server/server.js",
            "methods": ["GET"]
        },
        {
            "src": "/ping",
            "dest": "/server/server.js",
            "methods": ["GET"]
        },
        {
            "src": "/webhooks",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/graphql",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/metafield/remove/:id",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/variant/save/:productVariantId",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/variants/",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/variants/detail",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/upload/:productId",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/find/:sku",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/:productId/token/find",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/:productId/token/create",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/:productId/token/delete/:token",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/:productId",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/product/download/valid/:productId",
            "dest": "/server/server.js",
            "methods": ["GET"]
        },
        {
            "src": "/product/download/:productId",
            "dest": "/server/server.js",
            "methods": ["GET"]
        },
        {
            "src": "(/_next/static/.*)",
            "dest": "/server/server.js",
            "methods": ["GET"]
        },
        {
            "src": "/_next/webpack-hmr",
            "dest": "/server/server.js",
            "methods": ["GET"]
        },
        {
            "src": "(.*)",
            "dest": "/server/server.js",
            "methods": ["GET"]
        }
    ]
}
